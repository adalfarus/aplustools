[distutils]
  index-servers =
    pypi
    PROJECT_NAME
    testpypi

[pypi]
  username = __token__
  password = # either a user-scoped token or a project-scoped token you want to set as the default
[PROJECT_NAME]
  repository = https://upload.pypi.org/legacy/
  username = __token__
  password = # a project token
[testpypi]
  username = __token__
  password = # either a user-scoped token or a project-scoped token you want to set as the default
